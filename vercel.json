{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(newSession|completions|renameChat|deleteChat|history|conversation)",
      "dest": "/api/server.js"
    },
    { "src": "/coach(.*)", "dest": "/api/server.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
